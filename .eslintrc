{
    "extends": [
        "airbnb"
    ],
    "env": {
        "browser": true
    },
    "globals": {
        "__DEV__": false,
        "__PROD__": false
    },
    "rules": {
        "key-spacing": 0,
        "max-len": [2, 120, 2],
        "object-curly-spacing": [2, "always"],
        "react/jsx-indent": [2, 4],
        "indent": ["error", 4]
    }
}
